<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="160.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.idemia.tec.jkt.cardiotest.controller.ImportSettingsController">
  <children>
    <GridPane hgap="4.0" layoutX="14.0" layoutY="14.0" prefHeight="74.0" prefWidth="556.0" AnchorPane.leftAnchor="24.0" AnchorPane.rightAnchor="24.0" AnchorPane.topAnchor="24.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="160.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="360.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="40.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <children>
        <Label text="Project directory" />
        <TextField fx:id="txtProjectDir" GridPane.columnIndex="1" />
        <Button fx:id="btnProjectDir" mnemonicParsing="false" onAction="#handleButtonSelectProjectDir" prefWidth="32.0" GridPane.columnIndex="2" />
        <Label text="Adv save variables" GridPane.rowIndex="1" />
        <TextField fx:id="txtAdvSave" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Button fx:id="btnAdvSave" mnemonicParsing="false" onAction="#handleButtonSelectAdvSave" prefWidth="32.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      </children>
    </GridPane>
    <ButtonBar layoutX="380.0" layoutY="126.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="12.0" AnchorPane.rightAnchor="24.0">
      <buttons>
        <Button defaultButton="true" mnemonicParsing="false" onAction="#handleButtonOk" text="OK" />
        <Button mnemonicParsing="false" onAction="#handleButtonCancel" text="Cancel" />
      </buttons>
    </ButtonBar>
  </children>
</AnchorPane>
